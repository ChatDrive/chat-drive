{
  "name": "chat-drive",
  "private": true,
  "version": "0.0.0",
  "description": "A cloud storage platform",
  "type": "commonjs",
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "commit": "git add . && git cz",
    "changelog": "conventional-changelog -p gitmoji-config -i CHANGELOG.md -s -r 0",
    "eslint": "eslint src/** --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "format": "prettier --write src/**/*.{vue,js,ts,jsx,tsx,json,css} ",
    "stylelint": "stylelint src/**/*.{css,vue} --fix"
  },
  "dependencies": {
    "@vueuse/core": "^10.9.0",
    "colord": "^2.9.3",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.10",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.7",
    "vue": "^3.3.4",
    "vue-router": "4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@iconify/json": "^2.2.159",
    "@iconify/vue": "^4.1.1",
    "@types/crypto-js": "^4.2.2",
    "@types/lodash-es": "^4.17.12",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "@unocss/preset-uno": "^0.58.2",
    "@unocss/transformer-directives": "^0.58.2",
    "@unocss/vite": "^0.58.2",
    "@vitejs/plugin-vue": "^4.2.3",
    "commitizen": "^4.3.0",
    "commitlint-config-cz": "^0.13.3",
    "commitlint-config-git-commit-emoji": "^1.0.0",
    "conventional-changelog-cli": "^4.1.0",
    "conventional-changelog-gitmoji-config": "^1.5.2",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.51.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "naive-ui": "^2.36.0",
    "prettier": "^3.0.3",
    "rollup-plugin-visualizer": "^5.11.0",
    "stylelint": "^14.x",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^28",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-order": "^6.0.3",
    "typescript": "^5.0.2",
    "unplugin-icons": "^0.18.1",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^4.4.5",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-mock": "^3.0.0",
    "vite-plugin-progress": "^0.0.7",
    "vite-plugin-pwa": "^0.17.4",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-devtools": "1.0.0-rc.8",
    "vue-eslint-parser": "^9.3.2",
    "vue-tsc": "^1.8.5"
  }
}
